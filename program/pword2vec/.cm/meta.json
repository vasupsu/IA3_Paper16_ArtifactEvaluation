{
  "backup_data_uid": "838b3fef1a912e63",
  "build_compiler_vars": {
    "USE_MKL": ""
  },
  "compile_cmds": {
    "default": {
      "cmd": "../ck-compile$#script_ext#$"
    }
  },
  "compile_deps": {
    "compiler": {
      "local": "yes",
      "name": "C++ compiler",
      "tags": "compiler,lang-cpp,icc"
    }
  },
  "compiler_env": "CK_CXX",
  "data_name": "pword2vec",
  "main_language": "c",
  "process_in_tmp": "yes",
  "program": "yes",
  "run_cmds": {
    "default": {
      "ignore_return_code": "no",
      "run_time": {
        "run_cmd_main": "numactl --interleave=all $#BIN_FILE#$ -train  $<<CK_ENV_DATASET_TEXT8_FULL_NAME>>$ -output vectors.txt -size $<<CK_SIZE>>$ -window $<<CK_WINDOW>>$ -negative $<<CK_NEGATIVE>>$ -sample $<<CK_SAMPLE>>$ -threads $<<CK_THREADS>>$ -binary $<<CK_BINARY>>$ -iter $<<CK_ITER>>$ -min-count $<<CK_MIN_COUNT>>$ -batch-size $<<CK_BATCH_SIZE>>$",
        "run_input_files": [
          "text8"
        ]
      }
    }
  },
  "run_deps": {
    "dataset-text8l": {
      "force_target_as_host": "yes", 
      "local": "yes", 
      "name": "Dataset text8", 
      "sort": 40, 
      "tags": "dataset,text8"
    }
  }, 
  "run_vars": {
    "CK_SIZE":"10",
    "CK_WINDOW": "8",
    "CK_NEGATIVE": "5",
    "CK_SAMPLE":"1e-4",
    "CK_THREADS":"24",
    "CK_BINARY":"0",
    "CK_ITER":"10",
    "CK_MIN_COUNT":"5",
    "CK_BATCH_SIZE":"17"
  }, 
  "skip_bin_ext": "yes",
  "tags": [
    "demo",
    "program",
    "lang-cpp"
  ],
  "target_file": "pWord2Vec",
  "use_compile_script": "yes"
}
